{
  "version": 2,
  "builds": [
    {
      "src": "server/src/app.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/server/src/app",
      "dest": "server/src/app.js"
    },
    {
      "src": "/(.*)",
      "dest": "$1"
    }
  ],
  "crons": [
    {
      "path": "/api/cron",
      "schedule": "5 * * * *"
    }
  ]
}
