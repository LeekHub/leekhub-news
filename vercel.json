{
  "version": 2,
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/u/(?<id>[^/]*)",
      "dest": "api/index.js?id=$id"
    },
    {
      "src": "/(.*)",
      "dest": "$1"
    }
  ]
}
